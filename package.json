    "moduleNameMapper": {
      "^app(.*)$": "<rootDir>/src/containers/app$1",
      "^lib(.*)$": "<rootDir>/src/lib$1",
      "^assets(.*)$": "<rootDir>/src/assets$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^containers(.*)$": "<rootDir>/src/containers$1",
      "^login(.*)$": "<rootDir>/src/containers/login$1"
